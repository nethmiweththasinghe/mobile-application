<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context=".CommissionActivity"
android:background="@color/white"
android:orientation="vertical">

<TextView
    android:layout_width="227dp"
    android:layout_height="wrap_content"
    android:layout_marginLeft="80dp"
    android:layout_marginTop="50dp"
    android:fontFamily="sans-serif-black"
    android:text="Commision Rate "
    android:textColor="#FFA500"
    android:textSize="30dp" />

<TextView
    android:layout_width="227dp"
    android:layout_height="wrap_content"
    android:layout_marginLeft="10dp"
    android:layout_marginTop="30dp"
    android:fontFamily="sans-serif-black"
    android:text="Categories Rate"
    android:textColor="@color/black"
    android:textSize="20dp"/>
<LinearLayout
    android:layout_width="373dp"
    android:layout_height="55dp"
    android:layout_marginLeft="20dp"
    android:background="@drawable/buttton2"
    android:layout_marginTop="10dp"/>
<TextView
    android:layout_width="227dp"
    android:layout_height="wrap_content"
    android:layout_marginLeft="10dp"
    android:layout_marginTop="30dp"
    android:fontFamily="sans-serif-black"
    android:text="Percentage"
    android:textColor="@color/black"
    android:textSize="20dp"/>
<LinearLayout
    android:layout_width="373dp"
    android:layout_height="55dp"
    android:layout_marginLeft="20dp"
    android:background="@drawable/buttton2"
    android:layout_marginTop="10dp"/>

<Button
    android:layout_width="322dp"
    android:layout_height="50dp"
    android:layout_marginLeft="50dp"
    android:layout_marginTop="50dp"
    android:layout_marginRight="50dp"
    android:textSize="20dp"
    android:textAlignment="center"
    android:background="@drawable/text_bgs"
    android:text="Join Now" />

<ImageButton
    android:id="@+id/imageButton2"
    android:layout_width="298dp"
    android:layout_height="50dp"
    android:layout_marginLeft="60dp"
    android:layout_marginTop="60dp"
    android:background="@drawable/text_button"
    app:srcCompat="@drawable/img"/>

<ImageView
    android:id="@+id/imageView"
    android:layout_width="wrap_content"
    android:layout_height="44dp"
    android:layout_marginLeft="60dp"
    android:layout_marginTop="30dp"
    android:background="@drawable/gooogle"
    app:srcCompat="@drawable/google" />

<TextView
    android:layout_width="wrap_content"
    android:layout_height="30dp"
    android:layout_marginLeft="80dp"
    android:layout_marginTop="40dp"
    android:fontFamily="sans-serif-black"
    android:text="Already a member?"
    android:textSize="15dp">

</TextView>

</LinearLayout>
